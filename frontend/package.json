{
  "name": "book-club",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open --proxy-config proxy.conf.json",
    "start:dev": "concurrently --kill-others \"npm run mock:server\" \"npm run start:mock-proxy\"",
    "start:dpd": "ng serve --open --configuration=dpd",
    "build": "ng build",
    "test": "ng test --browsers=ChromeHeadlessCI",
    "lint": "ng lint && sass-lint -q -v",
    "sass-lint": "sass-lint -q -v",
    "e2e": "ng e2e"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm test"
    }
  },
  "private": true,
  "browserslist": [
    "defaults",
    "not IE 11",
    "not IE_Mob 11",
    "maintained node versions"
  ],
  "dependencies": {
    "@angular/animations": "~8.2.5",
    "@angular/common": "~8.2.5",
    "@angular/compiler": "~8.2.5",
    "@angular/core": "~8.2.5",
    "@angular/forms": "~8.2.5",
    "@angular/platform-browser": "~8.2.5",
    "@angular/platform-browser-dynamic": "~8.2.5",
    "@angular/router": "~8.2.5",
    "@casl/ability": "^3.4.0",
    "@casl/angular": "^2.1.1",
    "@ngrx/effects": "^8.3.0",
    "@ngrx/router-store": "^8.3.0",
    "@ngrx/store": "^8.3.0",
    "@ngrx/store-devtools": "^8.3.0",
    "@ngx-translate/core": "^12.1.2",
    "@ngx-translate/http-loader": "^4.0.0",
    "bootstrap": "^3.4.1",
    "core-js": "^2.5.4",
    "http-status-codes": "^1.3.2",
    "husky": "^3.1.0",
    "jquery": "^3.3.1",
    "lodash.isempty": "^4.4.0",
    "ng2-file-upload": "^1.4.0",
    "ngx-bootstrap": "^5.1.1",
    "rxjs": "~6.5.3",
    "tslib": "^1.9.0",
    "uuid": "^3.3.2",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~14.2.9",
    "@angular/cli": "~7.2.2",
    "@angular/compiler-cli": "~8.2.5",
    "@angular/language-service": "~8.2.5",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "concurrently": "^4.1.0",
    "jasmine-core": "~2.99.1",
    "jasmine-marbles": "^0.6.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "sass-lint": "^1.13.1",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.5.3"
  }
}
